<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:PopupPanel width="300" height="200" glassEnabled="true" modal="true" animationEnabled="true">
		<g:Grid width="100%" height="100%">
			<g:row>
				<g:customCell>
					<g:Label text="El nuevo USUARIO se ha crado con éxito" ui:field="mensaje"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="La ficha de inscripcion se enviara por correo" ui:field="mensaje2"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid width="100%" height="100%">
						<g:row>
							<g:customCell>
								<g:Label text="USUARIO:" width="150px"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="New Label" ui:field="usuario" width="100px"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label text="Contraseña" width="150px"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="New Label" ui:field="contrasennia" width="100px"/>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row align="center">
				<g:customCell>
					<g:Button text="Imprimir" styleName="btnppimprimir" ui:field="btnImprimir" width="71px"/>
				</g:customCell>
				<g:customCell>
					<g:Button text="Ok" styleName="btnOk" ui:field="btnOk" width="60px"/>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:PopupPanel>
</ui:UiBinder>
