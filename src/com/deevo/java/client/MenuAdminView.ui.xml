<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel width="200px" height="450px">
		<g:StackLayoutPanel width="100%" unit="PX" height="100%">
			<g:stack>
				<g:header size="36">Perfil</g:header>
				<g:Tree>
						<g:Label text="Ver Perfil"/>
						<g:Label text="Actulizar datos"/>
					</g:Tree>
			</g:stack>
			<g:stack>
				<g:header size="36">Usuarios</g:header>
				<g:Tree>
					<g:Label text="Nueva Persona"/>
					<g:Label text="Nuevo Usuario"/>
				</g:Tree>
			</g:stack>
			<g:stack>
				<g:header size="36">Alumnos</g:header>
				<g:Tree>
						<g:Label text="Nuevo Alumno" ui:field="nuevoAlumno"/>
						<g:Label text="Buscar Alumno"/>
					</g:Tree>
			</g:stack>
			<g:stack>
				<g:header size="36">Profesores</g:header>
				<g:Tree>
						<g:Label text="Nuevo Profesor"/>
						<g:Label text="Busqueda Profesor"/>
					</g:Tree>
			</g:stack>
			<g:stack>
				<g:header size="36">DeEvo</g:header>
				<g:Tree>
						<g:Label text="Inicio"/>
					</g:Tree>
			</g:stack>
		</g:StackLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder>
