<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel width="200px" height="450px">
		<g:StackLayoutPanel width="100%" unit="PX" height="100%">
			<g:stack>
				<g:header size="36">Perfil</g:header>
				<g:Tree>
						<g:Label text="Ver Perfil" ui:field="perfil"/>
						<g:Label text="Actulizar datos"/>
					</g:Tree>
			</g:stack>
			<g:stack>
				<g:header size="36">Usuarios</g:header>
				<g:Tree>
					<g:TreeItem text="Nuevo Usuario" state="true">
						<g:RadioButton name="Nuevo Usuario" text="Alumno" ui:field="alumnoOption"/>
						<g:RadioButton name="Nuevo Usuario" text="Padre de Familia" ui:field="padreOption"/>
						<g:RadioButton name="Nuevo Usuario" text="Profesor" ui:field="profesorOption"/>
						<g:RadioButton name="Nuevo Usuario" text="Psicologo" ui:field="psicologoOption"/>
					</g:TreeItem>
					<g:TreeItem text="Nuevo Rol" state="true">
						<g:Label text="Alumno"/>
						<g:Label text="Padre de Familia"/>
						<g:Label text="Profesor"/>
						<g:Label text="Psicologo"/>
					</g:TreeItem>
				</g:Tree>
			</g:stack>
			<g:stack>
				<g:header size="36">Alumnos</g:header>
				<g:Tree>
						<g:Label text="Nuevo Alumno" ui:field="nuevoAlumno"/>
						<g:Label text="Buscar Alumno"/>
					</g:Tree>
			</g:stack>
			<g:stack>
				<g:header size="36">Profesores</g:header>
				<g:Tree>
						<g:Label text="Nuevo Profesor"/>
						<g:Label text="Busqueda Profesor"/>
					</g:Tree>
			</g:stack>
			<g:stack>
				<g:header size="36">DeEvo</g:header>
				<g:Tree>
						<g:Label text="Inicio"/>
					</g:Tree>
			</g:stack>
		</g:StackLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder>
