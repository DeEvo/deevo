<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel width="100%" height="70px">
		<g:Grid width="980px" styleName="fondoInicioSesion" cellSpacing="1">
			<g:row>
				<g:customCell>
					<g:Image width="353px" height="60px"/>
				</g:customCell>
				<g:customCell>
					<g:HorizontalPanel width="167px" height="100%"/>
				</g:customCell>
				<g:customCell>
					<g:Grid width="420px" height="70px" styleName="layout" cellSpacing="1">
				<g:row>
					<g:customCell>
						<g:Label text="Usuario" width="163px" styleName="labelLogin" height="10px"/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Contraseña" styleName="labelLogin" height="10px"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:TextBox width="175px" styleName="textBoxLogin" height="15px"/>
					</g:customCell>
					<g:customCell>
						<g:PasswordTextBox width="175px" styleName="textBoxLogin" height="15px"/>
					</g:customCell>
					<g:customCell>
						<g:Button text="Entrar" ui:field="loginButton" styleName="botonInicio"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Recuperar contraseña" styleName="labelEnlace" height=""/>
					</g:customCell>
					<g:customCell>
						<g:Label text="Usuario Nuevo" styleName="labelEnlace"/>
					</g:customCell>
				</g:row>
			</g:Grid>
				</g:customCell>
			</g:row>
		</g:Grid>
	
	</g:HTMLPanel>
</ui:UiBinder>
