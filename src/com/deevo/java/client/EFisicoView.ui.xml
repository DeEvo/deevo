<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel styleName="fondoBienvenida" width="100%" height="100%">
		<g:Grid width="100%" styleName="cuadroBorde" height="100%">
			<g:row>
				<g:customCell>
					<g:Label text="Estado Físico" styleName="tituloFormulario"
						horizontalAlignment="ALIGN_CENTER" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Ficha de Usuario:" width="100%"
						styleName="labelsubformulario" height="20px" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid width="100%" cellPadding="6" styleName="layout">
						<g:row>
							<g:customCell>
								<g:Label text="DNI" styleName="labelFormulario" />
							</g:customCell>
							<g:customCell>
								<g:IntegerBox width="120px" ui:field="dniTexbox" enabled="false" />
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label text="Nombres" styleName="labelFormulario" />
							</g:customCell>
							<g:customCell>
								<g:TextBox width="200px" ui:field="nombresTexbox"
									enabled="false" />
							</g:customCell>
						</g:row>

						<g:row>
							<g:customCell>
								<g:Label text="Apellidos" styleName="labelFormulario" />
							</g:customCell>
							<g:customCell>
								<g:TextBox width="200px" ui:field="apellidosTexbox"
									enabled="false" />
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label text="DNI apoderado" width="194" styleName="labelFormulario" />
							</g:customCell>
							<g:customCell>
								<g:TextBox width="200px" enabled="false" />
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label height="20px" styleName="labelsubformulario" text="Aula:" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid width="100%" height="100%">
						<g:row>
							<g:customCell>
								<g:Label text="Nivel" width="60px" styleName="labelFormulario"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox width="120px" enabled="false"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Profesor" styleName="labelFormulario" width="60px"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox enabled="false" width="120px"/>
							</g:customCell>
						</g:row>
						<g:row>
							<g:customCell>
								<g:Label text="Grado" width="60px" styleName="labelFormulario"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox width="120px" enabled="false"/>
							</g:customCell>
							<g:customCell>
								<g:Label text="Sección" width="60px" styleName="labelFormulario"/>
							</g:customCell>
							<g:customCell>
								<g:TextBox width="120px" enabled="false"/>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label height="20px" styleName="labelsubformulario" text="Pruebas Fisicas" width="100%"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid width="100%" height="100%" styleName="layoutarriba"
						cellPadding="6">
						<g:row>
							<g:customCell>
								<g:Label text="Ver Historial" styleName="labelFormulario"/>
							</g:customCell>
							<g:customCell>
								<g:Button width="100px" text="Seleccionado" styleName="layout" />
							</g:customCell>
							<g:customCell>
								<g:Button width="100px" text="Todo" styleName="layout" />
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid width="100%" height="100%">
						<g:row>
							<g:customCell>
								<g:ScrollPanel height="96px">
									
									
								</g:ScrollPanel>
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Grid width="100%" height="100%" styleName="layoutarriba"
						cellPadding="6">
						<g:row>
							<g:customCell>
								<g:Button width="120px" text="Buscar Alumno" styleName="layout"
									ui:field="buscarButton" />
							</g:customCell>
							<g:customCell>
								<g:Button width="100px" text="Aceptar" styleName="layout"
									ui:field="crearButton" />
							</g:customCell>
							<g:customCell>
								<g:Button width="100px" text="Cancelar" styleName="layout" 
								ui:field="cancelarButton" />
							</g:customCell>
						</g:row>
					</g:Grid>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>

</ui:UiBinder>
