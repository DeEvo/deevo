<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">

	<g:HTMLPanel height="500">
	    <g:Grid width="100%">
	    	<g:row>
	    		<g:customCell>
	    			<g:Label text="Nuevo Persona" styleName="tituloFormulario" horizontalAlignment="ALIGN_CENTER"/>
	    		</g:customCell>
	    	</g:row>
	    	<g:row>
	    		<g:customCell>
	    			<g:Label text="Complete el siguiente formulario para inscribir a una persona en el sistema:" width="100%" styleName="labelsubformulario" height="37px"/>
	    		</g:customCell>
	    	</g:row>
	    	<g:row>
	    		<g:customCell>
	    			<g:Grid width="100%" cellPadding="6">
	    				<g:row>
	    					<g:customCell>
	    			<g:Label text="DNI*" styleName="labelFormulario"/>
	    		</g:customCell>
	    					<g:customCell>
	    						<g:IntegerBox width="120px" ui:field="dniTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Nombres*" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px" ui:field="nombresTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Apellido Paterno*" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px" ui:field="appaternTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Apellido Materno*" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px" ui:field="apmaternTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Estado Civil" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px" ui:field="estcivTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Fecha de nac.*" width="100%" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<p1:DateBox width="120px" ui:field="fnacTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Telefono" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:IntegerBox width="120px" ui:field="telfTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Celular" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:IntegerBox width="120px" ui:field="celTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="DirecciÃ³n" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="250px" ui:field="dirTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Email" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:IntegerBox width="200px" ui:field="emailTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    			</g:Grid>
	    		</g:customCell>
	    	</g:row>
	    	<g:row>
	    		<g:customCell>
	    			<g:Grid width="100%" height="100%" styleName="layoutarriba" cellPadding="10">
	    				<g:row>
	    					<g:customCell>
	    						<g:Button width="90px" text="Limpiar" styleName="layout"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:Button width="100px" text="Ingresar" styleName="layout" ui:field="ingresarButton"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:Button width="100px" text="Cancelar" styleName="layout"/>
	    					</g:customCell>
	    				</g:row>
	    			</g:Grid>
	    		</g:customCell>
	    	</g:row>
	    </g:Grid>
	</g:HTMLPanel>
	
</ui:UiBinder>
