<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default' xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">

	<g:HTMLPanel height="100%" width="100%" styleName="fondoBienvenida">
	    <g:Grid width="100%" height="100%" styleName="cuadroBorde">
	    	<g:row>
	    		<g:customCell>
	    			<g:Label text="Nuevo Usuario-Psicologo" styleName="tituloFormulario" horizontalAlignment="ALIGN_CENTER"/>
	    		</g:customCell>
	    	</g:row>
	    	<g:row>
	    		<g:customCell>
	    			<g:Label text="Complete el siguiente formulario para asignar un nuevo usuario-psicologo:" width="100%" styleName="labelsubformulario" height="37px"/>
	    		</g:customCell>
	    	</g:row>
	    	<g:row>
	    		<g:customCell>
	    			<g:Grid width="100%" cellPadding="6">
	    				<g:row>
	    					<g:customCell>
	    			<g:Label text="DNI" styleName="labelFormulario"/>
	    		</g:customCell>
	    					<g:customCell>
	    						<g:IntegerBox width="120px" ui:field="dniTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Nombres" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px" ui:field="nombresTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Usuario" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px" ui:field="usuarioTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="ContraseÃ±a" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px" ui:field="contraTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Colegio" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px" ui:field="colegioTexbox"/>
	    					</g:customCell>
	    				</g:row>
	    			</g:Grid>
	    		</g:customCell>
	    	</g:row>
	    	<g:row/>
	    	<g:row>
	    		<g:customCell>
	    			<g:Grid width="100%" height="100%" styleName="layoutarriba" cellPadding="10">
	    				<g:row>
	    					<g:customCell>
	    						<g:Button width="90px" text="Crear y salir" styleName="layout" ui:field="crearButton"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:Button width="116px" text="Crear y asignar aulas" styleName="layout" ui:field="asignarButton"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:Button width="100px" text="Cancelar" styleName="layout"/>
	    					</g:customCell>
	    				</g:row>
	    			</g:Grid>
	    		</g:customCell>
	    	</g:row>
	    </g:Grid>
	</g:HTMLPanel>
	
</ui:UiBinder>
