<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel width="980px" height="100%" styleName="layout">
		<g:DockPanel width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP" borderWidth="1">
			<g:Dock direction="NORTH" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
				<g:VerticalPanel width="100%" styleName="layout">
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:HTMLPanel ui:field="sesionContentPanel" height="70px">
						</g:HTMLPanel>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:HTMLPanel ui:field="headContentPanel" styleName="layout">
						</g:HTMLPanel>
					</g:Cell>
				</g:VerticalPanel>
			</g:Dock>
			<g:Dock direction="SOUTH">
				<g:HTMLPanel ui:field="footContentPanel" height="30px">
				</g:HTMLPanel>
			</g:Dock>
			<g:Dock direction="WEST">  
				<g:HTMLPanel ui:field="menuContentPanel" height="450px" width="110px"/>
			</g:Dock>
			<g:Dock direction="CENTER">  
				<g:HTMLPanel ui:field="mainContentPanel"  height="450px"/>
			</g:Dock>			
		</g:DockPanel>
	</g:HTMLPanel> 
</ui:UiBinder>
