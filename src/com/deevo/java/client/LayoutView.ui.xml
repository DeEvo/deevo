<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:HTMLPanel width="100%" height="100%">
		<g:DockPanel width="100%" height="100%" borderWidth="1" horizontalAlignment="ALIGN_CENTER">
			<g:Dock direction="NORTH">
				<g:HTMLPanel ui:field="topContentPanel" height="150px">
				Cabecera
				</g:HTMLPanel>
			</g:Dock>
			<g:Dock direction="WEST">
				<g:HTMLPanel ui:field="sideContentPanel" width="25px" height="100px">
				Costado
				</g:HTMLPanel>
			</g:Dock>
			<g:Dock direction="CENTER">  
				<g:HTMLPanel ui:field="mainContentPanel"  height="100px">
				</g:HTMLPanel>
			</g:Dock>			
		</g:DockPanel>
	</g:HTMLPanel> 
</ui:UiBinder>
