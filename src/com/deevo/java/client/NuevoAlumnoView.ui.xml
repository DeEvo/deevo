	    	</g:row>
	    	<g:row>
	    		<g:customCell>
	    			<g:Label text="Complete el siguiente formulario para inscribir a una persona en el sistema:" width="100%" styleName="labelsubformulario" height="37px"/>
	    		</g:customCell>
	    	</g:row>
	    	<g:row>
	    		<g:customCell>
	    			<g:Grid width="100%" cellPadding="6">
	    				<g:row>
	    					<g:customCell>
	    			<g:Label text="DNI*" styleName="labelFormulario"/>
	    		</g:customCell>
	    					<g:customCell>
	    						<g:IntegerBox width="120px"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Nombres*" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Apellido Paterno*" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Apellido Materno*" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Fecha de nac.*" width="100%" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<p1:DateBox width="120px" fireNullValues="true"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Telefono*" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:IntegerBox width="120px"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Celular" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:IntegerBox width="120px"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="DirecciÃ³n" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px"/>
	    					</g:customCell>
	    				</g:row>
	    				<g:row>
	    					<g:customCell>
	    						<g:Label text="Email" styleName="labelFormulario"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:TextBox width="200px"/>
	    					</g:customCell>
	    				</g:row>
	    			</g:Grid>
	    		</g:customCell>
	    	</g:row>
	    	<g:row>
	    		<g:customCell>
	    			<g:Grid width="100%" height="100%" styleName="layoutarriba" cellPadding="10">
	    				<g:row>
	    					<g:customCell>
	    						<g:Button width="90px" text="Limpiar" styleName="layout"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:Button width="100px" text="Ingresar" styleName="layout"/>
	    					</g:customCell>
	    					<g:customCell>
	    						<g:Button width="100px" text="Cancelar" styleName="layout"/>
	    					</g:customCell>
	    				</g:row>
	    			</g:Grid>
	    		</g:customCell>
	    	</g:row>
	    </g:Grid>
	</g:HTMLPanel>
	
</ui:UiBinder>
